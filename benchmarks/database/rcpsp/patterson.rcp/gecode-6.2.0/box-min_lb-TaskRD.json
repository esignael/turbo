{
  "problem_set_path":
    "../../absolute-project/kobe-rcpsp/data/rcpsp/patterson.rcp/",
  "timeout": 60,
  "solver_instance": [
    "FznSolver",
    {
      "fzn": {
        "solver": {
          "name": "gecode",
          "version": "6.2.0",
          "exec":
            "fzn-gecode",
          "globals":
            "/home/ptalbot/repositories/resources/solvers/gecode/gecode/flatzinc/mznlib"
        },
        "mzn2fzn": {
          "name": "mzn2fzn",
          "version": "2.3.1",
          "exec": "minizinc",
          "globals": ""
        },
        "option": "None",
        "strategy": {
          "plain": "smallest, indomain_min, complete",
          "short": "min_lb"
        }
      },
      "decompositions": [
        {
          "name": "TaskRD",
          "description": "Task-resource decomposition of cumulative"
        }
      ]
    }
  ],
  "csv": {
    "fields": [
      "ProblemName",
      "Nodes",
      "Solutions",
      "Fails",
      [ "Time", "Sec" ],
      "Optimum"
    ]
  }
}
